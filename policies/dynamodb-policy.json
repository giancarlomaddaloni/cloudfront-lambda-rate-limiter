{
    "Version": "2012-10-17",
    "Statement": [

      { "Sid":"Dynamo DB Registries",
        "Effect": "Allow",
        "Action": [
          "dynamodb:Query",
          "dynamodb:GetItem",
          "dynamodb:UpdateItem",
          "dynamodb:PutItem",
          "dynamodb:DeleteItem"
        ],
        "Resource": "*"
      }
    ]
  }
  